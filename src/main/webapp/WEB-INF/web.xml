<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <!--Put the servlet block for each servlet, in the order of insert, update, and delete-->
    <servlet>
        <servlet-name>insertMembers</servlet-name>
        <servlet-class>com.uh.rachel.util.servlets.insertMembers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateMembers</servlet-name>
        <servlet-class>com.uh.rachel.util.servlets.updateMembers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteMembers</servlet-name>
        <servlet-class>com.uh.rachel.util.servlets.deleteMembers</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>loginVal</servlet-name>
        <servlet-class>com.uh.rachel.util.servlets.loginVal</servlet-class>
    </servlet>


    <!--Put the servlet-mapping for each servlet, in the order of insert, update, and delete-->
    <servlet-mapping>
        <servlet-name>insertMembers</servlet-name>
        <url-pattern>/insertMembers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateMembers</servlet-name>
        <url-pattern>/updateMembers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteMembers</servlet-name>
        <url-pattern>/deleteMembers</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>insertEvents</servlet-name>
        <servlet-class>com.uh.rachel.util.servlets.insertEvents</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateEvents</servlet-name>
        <servlet-class>com.uh.rachel.util.servlets.updateEvents</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteEvents</servlet-name>
        <servlet-class>com.uh.rachel.util.servlets.deleteEvents</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>insertEvents</servlet-name>
        <url-pattern>/insertEvents</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateEvents</servlet-name>
        <url-pattern>/updateEvents</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteEvents</servlet-name>
        <url-pattern>/deleteEvents</url-pattern>
    </servlet-mapping>














</web-app>